         </Switch>\n              </div>\n            </div>\n          </Router>\n        </ProvideTeam>\n      </ProvideAuth>\n    </ProvideDialog>\n  )\n}\n\nconst PrivateRoute = ({ children, ...rest }) => {\n  const { user, fetchingCacheUser } = useAuth()\n  if (fetchingCacheUser) {\n    return <Loader />\n  } else {\n    return (\n      <Route\n        {...rest}\n        render={({ location }) =>\n          user ? (\n            <MainLayout>{children}</MainLayout>\n          ) : (\n            <Redirect\n              to={{\n                pathname: '/login',\n                state: { from: location }\n              }}\n            />\n          )\n        }\n      />\n    )\n  }\n}\n","/Users/felipe/projects/Gamificacion-equipo1/src/hooks/index.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/dialogs/signOut.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Loader.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/layouts/mainLayout.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Avatar.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Spaceship.js",["107"],"/Users/felipe/projects/Gamificacion-equipo1/src/index.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/ParticleBackground.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/pages/Dashboard.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Missions.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/index.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Progress.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/hooks/user.js",["108","109"],"import firebase from 'firebase/app'\nimport React, { createContext, useContext, useState, useEffect } from 'react'\nimport 'firebase/auth'\nimport { useHistory } from 'react-router-dom'\nimport { db } from '../index'\nimport { useDialog } from './'\nconst authContext = createContext()\nconst googleProvider = new firebase.auth.GoogleAuthProvider()\n\nconst useProvideAuth = () => {\n  const [loading, setLoading] = useState(false)\n  const [fetchingCacheUser, setFetchingCacheUser] = useState(true)\n  const [user, setUser] = useState(null)\n  const [player, setPlayer] = useState(null)\n  const [error, setError] = useState(null)\n  const history = useHistory()\n  const { openDialog } = useDialog()\n  const onAuthStateChanged = () => {\n    return firebase.auth().onAuthStateChanged(user => {\n      if (user) {\n        setUser({\n          displayName: user.displayName,\n          photoURL: user.photoURL,\n          uid: user.uid\n        })\n      }\n      setFetchingCacheUser(false)\n    })\n  }\n\n  useEffect(() => {\n    let playerConextion = () => {}\n    if (user) {\n      const playerCollection = db.collection('players').doc(user.uid)\n      playerConextion = playerCollection.onSnapshot(player => {\n        if (player.data()) {\n          setPlayer({ ...player.data() })\n        } else {\n          playerCollection\n            .set({\n              displayName: user.displayName,\n              photoURL: user.photoURL\n            })\n            .then(resp => {\n              console.log(resp)\n            })\n        }\n      })\n    }\n    return () => {\n      playerConextion()\n    }\n  }, [user])\n\n  useEffect(() => {\n    let mounted = true\n    const unsubscribe = mounted && onAuthStateChanged()\n    return () => {\n      mounted = false\n      unsubscribe()\n    }\n  }, [])\n\n  useEffect(() => {\n    if (player && !player.teamId) {\n      openDialog('selectTeam', {\n        shouldCloseOnOverlayClickbool: false,\n        shouldCloseOnEscapePressbool: false\n      })\n    }\n  }, [player])\n\n  const signin = async () => {\n    try {\n      setLoading(true)\n      const result = await firebase.auth().signInWithPopup(googleProvider)\n      setUser({\n        displayName: result.user.displayName,\n        photoURL: result.user.photoURL,\n        uid: result.user.uid\n      })\n      setLoading(false)\n      history.push('/')\n    } catch (error) {\n      setError(error)\n      setLoading(false)\n    }\n  }\n  const signout = () => {\n    console.log('singing out')\n    firebase\n      .auth()\n      .signOut()\n      .then(() => {\n        setUser(null)\n        history.push('/login')\n      })\n      .catch(error => {\n        setError(error)\n      })\n  }\n  return {\n    user,\n    player,\n    fetchingCacheUser,\n    loading,\n    signin,\n    signout\n  }\n}\n\nconst ProvideAuth = ({ children }) => {\n  const auth = useProvideAuth()\n  return <authContext.Provider value={auth}>{children}</authContext.Provider>\n}\n\nconst useAuth = () => {\n  return useContext(authContext)\n}\n\nexport { useProvideAuth, ProvideAuth, useAuth }\n",["110","111"],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Dialog.js",["112"],"/Users/felipe/projects/Gamificacion-equipo1/src/dialogs/Teams.js",["113"],"import React, { useEffect, useState } from 'react'\nimport Box from 'ui-box'\nimport { Heading, Button, Select } from 'evergreen-ui'\nimport { useTeam } from '../hooks'\n\nconst Teams = props => {\n  const [teamSelected, setSelectedTeam] = useState(null)\n  const { teams, getTeams, saveTeamSelected } = useTeam()\n  useEffect(() => {\n    getTeams()\n  }, [])\n  const onClick = () => saveTeamSelected(teamSelected)\n  return (\n    <Box display='flex' flexDirection='column'>\n      <Heading size={100} marginTop='default'>\n        Selecciona tu equipo\n      </Heading>\n      <Select flex={1} onChange={event => setSelectedTeam(event.target.value)}>\n        <option hidden={true}>Selecciona tu equipo</option>\n        {teams.map(team => (\n          <option\n            key={team.id}\n            value={team.id}\n            defaultValue={team.id === teamSelected}\n          >\n            {team.name}\n          </option>\n        ))}\n      </Select>\n      <Box\n        margin={10}\n        display='flex'\n        flex={1}\n        flexDirection='row'\n        justifyContent='flex-end'\n      >\n        <Button disabled={!teamSelected} intent='success' onClick={onClick}>\n          Guardar\n        </Button>\n      </Box>\n    </Box>\n  )\n}\n\nexport { Teams }\n","/Users/felipe/projects/Gamificacion-equipo1/src/hooks/teams.js",["114","115","116","117"],"/Users/felipe/projects/Gamificacion-equipo1/src/components/PlayerAvatar.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/pages/Experiment.js",["118","119"],"/Users/felipe/projects/Gamificacion-equipo1/src/layouts/index.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Rocket.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Crew.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Tasks.js",["120","121"],"/Users/felipe/projects/Gamificacion-equipo1/src/components/Task.js",[],"/Users/felipe/projects/Gamificacion-equipo1/src/hooks/tasks.js",["122","123","124"],{"ruleId":"125","severity":1,"message":"126","line":9,"column":17,"nodeType":"127","messageId":"128","endLine":9,"endColumn":26},{"ruleId":"125","severity":1,"message":"129","line":2,"column":39,"nodeType":"127","messageId":"128","endLine":2,"endColumn":48},{"ruleId":"125","severity":1,"message":"130","line":15,"column":10,"nodeType":"127","messageId":"128","endLine":15,"endColumn":15},{"ruleId":"131","severity":1,"message":"132","line":71,"column":6,"nodeType":"133","endLine":71,"endColumn":14,"suggestions":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"125","severity":1,"message":"139","line":6,"column":7,"nodeType":"127","messageId":"128","endLine":6,"endColumn":11},{"ruleId":"131","severity":1,"message":"140","line":11,"column":6,"nodeType":"133","endLine":11,"endColumn":8,"suggestions":"141"},{"ruleId":"125","severity":1,"message":"142","line":10,"column":19,"nodeType":"127","messageId":"128","endLine":10,"endColumn":29},{"ruleId":"125","severity":1,"message":"130","line":12,"column":10,"nodeType":"127"